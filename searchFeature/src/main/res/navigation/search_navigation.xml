<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/search_navigation"
    app:startDestination="@id/search">

    <fragment
        android:id="@+id/search"
        android:name="com.yes.searchfeature.presentation.ui.SearchScreen"
        android:label="@string/menu"
        tools:layout="@layout/search" >

        <deepLink
            app:uri="android-app://com.yes.searchfeature.presentation.ui.SearchScreen/{departure}" />
        <argument
            android:name="departure"
            app:argType="string" />
    <action
        android:id="@+id/openThirdFeatureAction"
        app:destination="@id/search_stub"
        app:enterAnim="@anim/nav_default_enter_anim"
        app:exitAnim="@anim/nav_default_exit_anim"
        app:popEnterAnim="@anim/nav_default_pop_enter_anim"
        app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>
    <fragment
        android:id="@+id/search_stub"
        android:name="com.yes.searchfeature.presentation.ui.StubScreen"
        android:label="@string/stub"
        tools:layout="@layout/stub" />



</navigation>